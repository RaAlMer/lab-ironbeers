<div class="col-md-4">
  <div>
    {{#each beerInfo}}
    {{> beerpartial this}}
    <h6><b>ABV: </b>{{abv}}</h6>
    <h6><b>IBU: </b>{{ibu}}</h6>
    <h6><b>Food Pairing</b></h6>
    <ul>
        {{#each food_pairing}}
        <li>{{this}}</li>
        {{/each}}
    </ul>
    <p><b>Brewers Tips: </b>{{brewers_tips}}</p>
    {{/each}}
  </div>
</div>